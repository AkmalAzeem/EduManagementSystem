<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="context" prefHeight="252.0" prefWidth="510.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.developersstack.edumanagement.controller.ResetPasswordFormController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Forgot Password?" textFill="#2c04f2">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button layoutX="60.0" layoutY="141.0" mnemonicParsing="false" onAction="#changePasswordOnAction" prefHeight="43.0" prefWidth="354.0" text="Change Password">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <PasswordField fx:id="txtPassword" layoutX="60.0" layoutY="76.0" prefHeight="43.0" prefWidth="373.0" promptText="New Password" />
   </children>
</AnchorPane>
